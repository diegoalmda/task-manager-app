{
  "expo": {
    "scheme": "task-manager",
    "name": "task-manager",
    "slug": "task-manager",
    "version": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.todolist",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1F4287"
      }
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1F4287"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ]
  }
}
